extends caseLayout

block case-content
  .case-content
    form.form-horizontal(action=`/commit/${caseId}` method="POST")
      input.btn.btn-default.yuda-input-btn(type="submit" disabled=!finished value=buttonConfig.buttons.commit.text)

    each item in configs
      .row
        .col-sm-4
          span #{item.text}
        .col-sm-8
          span #{item.message}
