{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-console": "off",
    "max-len": "off",
    "brace-style": ["warn", "stroustrup"]
  }
}
